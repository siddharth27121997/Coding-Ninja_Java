
public class Prime {

	
	public static void prime_no(int n)
	{
		int d=2;
		boolean div= false;
		while(d<n)
		{
		if(n%d==0)
		{
			div=true;
		    break;	
		}
		d++;
		}
		if(n==1 || n==0)
		{
			div=true;
		}
		if(div==false)
		{
	    	System.out.println(n);	
		}
	}
			
		
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	//	int n=10;
		for(int i=0;i<1000;i++)
		{
			prime_no(i);
		}
	//System.out.println(x);
	}

}
