import java.util.*;
public class Pattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		

		int n;
		Scanner sc = new Scanner(System.in);
   n= sc.nextInt();
   int i=1;
   int var=1;
   while(i<=n)
   {
	   int j=1;
	   while(j<=i)
	   {
		   System.out.print(var);
		   var=var+1;
		   j++;
	   }
		   System.out.println(" /t ");
		   i++;
	   
   }
   
   
   
	}

}
