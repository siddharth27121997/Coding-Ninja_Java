import java.util.*;
public class FarenTocel {

	public static int far(int s,int e,int w)
	{
		int f =s;
		int c=1;
		while(f<=e)
		{
			 c =(int)(5.0/9)*(f-32);
		//	System.out.println(f+"\t"+c);
			f=f+w;
		//return c;
		}
		
		return c;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner sc = new Scanner(System.in);
	int s=sc.nextInt();
	int e=sc.nextInt();
	int w = sc.nextInt();
	int car=far(s,e,w);
	System.out.println(car);
	}

}
