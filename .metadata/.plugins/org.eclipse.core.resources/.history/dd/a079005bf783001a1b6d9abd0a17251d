import java.util.*;
public class Pattern1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n;
		Scanner sc = new Scanner(System.in);
   n= sc.nextInt();
   int i=1;
   while(i<=n)
   {
	   int j=1;
	   while(j<=i)
	   {
		   System.out.print(j);
		   j=j*2;
	   }
		   System.out.println();
		   i++;
	   
   }
   
   
   
	}

}
